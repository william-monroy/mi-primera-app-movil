{
  "expo": {
    "name": "MLogic",
    "slug": "MLogic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/back.png",
      "resizeMode": "contain",
      "backgroundColor": "#808080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.william.mlogic",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.william_monroy.MLogic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "40.0.0", "platforms": [ "ios", "android" ]
  }
}
